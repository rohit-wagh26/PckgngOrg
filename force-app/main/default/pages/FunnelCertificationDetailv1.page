    <script>

        function show(obj)
        {
            if(document.getElementById(obj).style.display == 'none')
            {
                document.getElementById(obj).style.display = 'block';
                if(obj == "expand") {
                  document.getElementById(obj).previousSibling.previousSibling.innerHTML = "<i style='font-size:20px' class='fas'>&#xf107;</i> Monthly";
                }
                else if(obj == "expand1") {
                  document.getElementById(obj).previousSibling.previousSibling.innerHTML = "<i style='font-size:20px' class='fas'>&#xf107;</i> Quarterly";
                }
                else if(obj == "expand2") {
                  document.getElementById(obj).previousSibling.previousSibling.innerHTML = "<i style='font-size:20px' class='fas'>&#xf107;</i> YTD";
                }
                else if(obj == "expand3") {
                  document.getElementById(obj).previousSibling.previousSibling.innerHTML = "<i style='font-size:20px' class='fas'>&#xf107;</i> 12 Month Rolling";
                }
            }
            else {
                if(obj == "expand") {
                  document.getElementById(obj).previousSibling.previousSibling.innerHTML = "<i style='font-size:20px' class='fas'>&#xf105;</i> Monthly";
                }
                else if(obj == "expand1") {
                  document.getElementById(obj).previousSibling.previousSibling.innerHTML = "<i style='font-size:20px' class='fas'>&#xf105;</i> Quarterly";
                }
                else if(obj == "expand2") {
                  document.getElementById(obj).previousSibling.previousSibling.innerHTML = "<i style='font-size:20px' class='fas'>&#xf105;</i> YTD";
                }
                else if(obj == "expand3") {
                  document.getElementById(obj).previousSibling.previousSibling.innerHTML = "<i style='font-size:20px' class='fas'>&#xf105;</i> 12 Month Rolling";
                }
                document.getElementById(obj).style.display = 'none';
